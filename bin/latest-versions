#!/bin/bash
source `dirname $0`/config.sh

if [ -z $1 ]; then
    java -jar $LEVI_PATH list-versions --url $ARTIFACTORY_URL --repo $ARTIFACTORY_REPO --user $ARTIFACTORY_USER --password "\"$ARTIFACTORY_PASSWORD\""
else
    java -jar $LEVI_PATH list-versions --url $ARTIFACTORY_URL --repo $1 --user $ARTIFACTORY_USER --password "\"$ARTIFACTORY_PASSWORD\""
fi
